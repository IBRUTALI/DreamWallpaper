<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ActionBarTheme" parent="@style/Widget.MaterialComponents.Toolbar">
        <item name="android:backgroundTint">?attr/colorPrimary</item>
        <item name="android:background">?attr/colorOnPrimary</item>
        <item name="android:titleTextStyle">@style/Toolbar.TitleText</item>
        <item name="android:tint">?attr/colorPrimaryVariant</item>
        <item name="android:textColorPrimary">?attr/colorSecondary</item>
    </style>

    <style name="Toolbar.TitleText" parent="@style/TextAppearance.MaterialComponents.Headline6">
        <item name="android:fontFamily">@font/tt_norms_regular</item>
        <item name="fontFamily">@font/tt_norms_regular</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="AlertDialogCustom" parent="Theme.AppCompat.DayNight.Dialog.Alert">
        <!-- buttons -->
        <item name="colorAccent">@color/white</item>
        <!-- title and text -->
        <item name="android:textColorPrimary">@color/white</item>
        <!-- background you can change some other color than white -->
        <item name="android:background">@color/blackLite_200</item>
        <!-- change if you want below -->
        <item name="android:textColor">@color/white</item>
    </style>

</resources>